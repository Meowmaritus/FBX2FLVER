<Window x:Class="FBX2FLVER.PresetInfoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FBX2FLVER"
        mc:Ignorable="d"
        Title="Preset '?PresetName?' Details" Height="600" Width="800" WindowStartupLocation="CenterOwner" ResizeMode="NoResize" ShowInTaskbar="False" > 
    <Grid>
        <Button x:Name="ButtonOK" HorizontalAlignment="Right" VerticalAlignment="Bottom" Height="20" Width="111" Margin="0,0,4,4" Content="OK" Click="ButtonOK_Click"/>
        <TextBlock Name="TextBlockDetails" HorizontalAlignment="Stretch" TextWrapping="Wrap" Text="?PresetInfo?" VerticalAlignment="Stretch" Margin="8,8,8,29" FontFamily="Consolas"/>
    </Grid>
</Window>
